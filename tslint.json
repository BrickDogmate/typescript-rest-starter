{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "eofline": false,
    "max-line-length":  [true, 240],
    "strict-boolean-expressions": false,
    "no-else-after-return": false,
    "no-unused-variable": false,
    "no-unsafe-any": false,
    "prefer-template": false
  }
}
